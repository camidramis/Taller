def imagenx(R,x):
    img_x=set()
    etre='No está en la relación'
    if x not in dominioNatural(R):
        return etre
    else:
        for (z,y) in R:
            if x==z: 
                img_x.add(y)
    print(img_x)
#imagenx(pc,'a')

def transitividad(R):
    x = dominioNatural(R)
    for y in imagenx(R,x):
        for z in imagenx(R,y):
            if z in imagenx(R,x):
                transitividad = True
            else:
                transitividad = False
    return transitividad

transitividad(R)

def esAntisimetrica(R):
    for (x,y) in R:
        if (y,x) in R:
            if x==y:
                return True
        return False

A={1,2,3}
B={1,2,3}

R=productoCartesiano(A,B)
print(R)
esAntisimetrica(R)
